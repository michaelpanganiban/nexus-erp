{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "next/core-web-vitals"
    // Add other configurations if needed
  ],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "quotes": ["error", "single"],
    "no-unused-vars":"warn",
    "prefer-const":"warn",
    "space-before-blocks":"warn",
    "space-in-parens":"warn",
    "space-before-function-paren":"warn"
    // Add other rules as needed
  }
}